<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:repository="clr-namespace:PortalDoFranqueado.Repository"
                    xmlns:converter="clr-namespace:PortalDoFranqueado.Util.Converter">
    <repository:TemporaryLocalRepository x:Key="TempCache"/>
    <repository:PersistentLocalRepository x:Key="PersistCache"/>
    <converter:EnumConverter x:Key="EnumConverter"/>
    <converter:PurchaseItemGroupDescriptionConverter x:Key="PurchaseItemGroupDescriptionConverter"/>
    <converter:ExpandableConverter x:Key="ExpandableConverter"/>
    <converter:ReverseBooleanConverter x:Key="ReverseBooleanConverter"/>
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Style.xaml"/>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>